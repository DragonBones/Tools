{
	"compilerOptions": {
		"sourceMap": false,
		"declaration": false,
		"skipLibCheck": true,
		"alwaysStrict": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"target": "es5",
		"module": "commonjs",
		"newLine": "LF",
		"baseUrl": "./src",
		"outDir": "./out",
		"lib": [
			"es6",
			"dom",
			"es2017"
		]
	},
	"exclude": [
		"node_modules",
		"out",
		"backup"
	]
}